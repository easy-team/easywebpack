{
  "name": "@easy-team/res-cli",
  "version": "3.0.0",
  "description": "Node React Web Framework CLI",
  "bin": {
    "res": "bin/cli.js"
  },
  "keywords": [
    "npm",
    "npm package",
    "unit test",
    "eslint",
    "travis",
    "travis-ci",
    "code coverage",
    "changelog"
  ],
  "dependencies": {
    "easywebpack-cli": "^5.0.0",
    "easywebpack-react": "^5.0.0",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "egg-bin": "^4.13.2",
    "egg-scripts": "^2.11.0",
    "egg-webpack": "^5.0.0",
    "egg-webpack-react": "^3.0.0",
    "eslint-config-egg": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.0",
    "node-tool-utils": "^1.1.1",
    "ts-loader": "^6.0.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "codecov": "^3.0.0",
    "conventional-changelog-cli": "^1.3.5",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0",
    "eslint-config-eslint": "^4.0.0",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "test": "egg-bin test",
    "cov": "egg-bin cov",
    "ci": "npm run lint && npm run cov",
    "ii": "npm install --registry https://registry.npm.taobao.org"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "test/*.test.js"
    ]
  },
  "ci": {
    "version": "10, 12, 14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easy-team/res-cli.git"
  },
  "author": "sky"
}
